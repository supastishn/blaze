{
  "name": "ts2cpp",
  "version": "1.0.1",
  "description": "TypeScript to C++ Compiler",
  "main": "dist/compiler/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test-project": "node --no-warnings -r ts-node/register src/cli/index.ts compile example.ts -o example.cpp",
    "dev": "node --no-warnings -r ts-node/register src/cli/index.ts",
    "bundle": "esbuild src/cli/index.ts --bundle --platform=node --outfile=dist/sea-bundle.js",
    "package": "npm run build && npm run bundle && node scripts/build-sea.js",
    "get-deps": "node scripts/get-node-binaries.js"
  },
  "bin": {
    "ts2cpp": "dist/cli/index.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.0",
    "esbuild": "^0.23.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "postject": "^1.0.0-alpha.6"
  }
}
